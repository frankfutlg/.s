! xft behavior
Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault
Xcursor.theme: phinger-cursors
Xcursor.size: 32
Xft.dpi: 96

! window padding
st.borderpx: 16

! terminal
st.font:           cherry:size=11

